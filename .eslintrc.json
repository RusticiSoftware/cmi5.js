{
    "env": {
        "browser": true,
        "es2020": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": "@rusticisoftware/eslint-config",
    "rules": {
        "no-magic-numbers": "off",
        "one-var": [
            "error",
            {
                "var": "always",
                "let": "consecutive",
                "const": "consecutive"
            }
        ]
    }
}
